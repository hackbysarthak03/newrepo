{
    "builds": [{
        "src": "newproject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/productionfiles/(.*)",     
            "dest": "/productionfiles/$1" 
        },
        {
            "src": "/(.*)",
            "dest": "newproject/wsgi.py"
        }
    ]
}